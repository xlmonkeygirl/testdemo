    if(!keyword){
      return false;
    }
    keyword = encodeURI(keyword);
    window.location.href = "https://www.cnrmall.com/search?keyword=" + keyword;
  });

    $("#buttonSearchStore").on("click",function(){
        var keyword = $("#keywordStore").val();
        keyword = encodeURI(keyword);
        window.location.href = "https://www.cnrmall.com/search/store?keyword=" + keyword;
    });

    // 监听搜索框中的input
